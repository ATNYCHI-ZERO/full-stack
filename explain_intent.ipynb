   "source": [
    "import joblib\n",
    "import pandas as pd\n",
    "import shap\n",
    "model = joblib.load('intent_model.pkl')\n",
    "X = pd.read_csv('intent_dataset.csv').drop('intent', axis=1)\n",
    "explainer = shap.Explainer(model, X)\n",
    "values = explainer(X)\n",
    "shap.summary_plot(values, X)"
   ]
  }
